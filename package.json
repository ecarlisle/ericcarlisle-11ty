    "@11ty/eleventy-plugin-syntaxhighlight": "^4.2.0",
    "@slinkity/renderer-react": "^1.0.0",
    "@types/react": "^18.0.25",
    "autoprefixer": "^10.4.13",
    "browserslist": "^4.21.4",
    "cssnano": "^5.1.14",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
