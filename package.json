{
  "name": "www.ericcarlisle.com",
  "version": "4.0.0",
  "description": "Website for Eric Carlisle : Web Developer, UX Strategist",
  "main": ".eleventy.js",
  "scripts": {
    "build": "rm -rf _site && npx @11ty/eleventy -- --config .eleventy.js",
    "clean": "rm -rf _site",
    "debug": "DEBUG=Eleventy* npx @11ty/eleventy --serve --incremental --config .eleventy.js --port 3000",
    "watch": "npx @11ty/eleventy --serve --incremental --config .eleventy.js --port 3000",
    "watch:prod": "MODE=production npx @11ty/eleventy --serve --incremental --quiet --config .eleventy.js --port 3000"
  },
  "author": "",
